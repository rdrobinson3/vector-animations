<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="500dp"
    android:height="500dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <group android:name="rotationGroup"
        android:pivotX="50.0"
        android:pivotY="50.0"
        android:rotation="0">

        <path
            android:name="top"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapTop" />


        <path
            android:name="left"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapLeft" />


        <path
            android:name="middleLeft"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapMiddleLeft" />


        <path
            android:name="middleRight"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapMiddleRight" />

        <path
            android:name="right"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapRight" />


        <path
            android:name="bottom"
            android:fillColor="#D0011B"
            android:strokeWidth="1"
            android:pathData="@string/mapBottom" />

    </group>


</vector>